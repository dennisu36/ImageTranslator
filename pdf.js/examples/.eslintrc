{
  "extends": [
    "../.eslintrc"
  ],

  "parserOptions": {
    "ecmaVersion": 5,
  },

  "env": {
    "es6": false,
  },

  "globals": {
    "pdfjsImageDecoders": false,
    "pdfjsLib": false,
    "pdfjsViewer": false,
    "Uint8Array": false,
  },

  "rules": {
    "object-shorthand": ["error", "never"]
  }
}
